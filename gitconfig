[color]
	ui = true
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

  [color "status"]
    added = yellow
    changed = green
    untracked = cyan

  [color "diff"]
    whitespace = red reverse

[alias]
	co = checkout
	st = status
	br = branch
	df = diff
	pl = pull
	ps = push

